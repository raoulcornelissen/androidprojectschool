<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.congreats.stoppolitie">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo512"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutActivity"
            android:label="@string/aboutheadertext">
        </activity>
        <activity android:name=".HelpActivity"
            android:label="@string/helpheadertext">
        </activity>
        <activity android:name=".ContactActivity"
            android:label="@string/contactheadertext">
        </activity>
        <activity android:name=".GetKentekenActivity"
            android:label="@string/kentekenopvragenheadertext">
        </activity>
        <activity android:name=".AddKentekenActivity"
            android:label="@string/kentekentoevoegenheadertext">
        </activity>
        <activity android:name=".ScannerActivity"
                  android:label="scanner">
        </activity>
    </application>

</manifest>